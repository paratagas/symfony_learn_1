<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block custom %}{% endblock %}
        {% block doctrine %}{% endblock %}
        {% block doctrine_dql %}{% endblock %}
        {% block doctrine_dqb %}{% endblock %}
        {% block doctrine_drep %}{% endblock %}
        {% block doctrine_drelated %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
